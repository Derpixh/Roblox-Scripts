local buttonsTab = window:CreateTab("Buttons", 4483362458)
local scriptSectionButtons = buttonsTab:CreateSection("Script")
local firedClickDetectors = {}
local shouldContinueChecking = true
local function checkAndFireClickDetectors()
    local buttonCompetitionModel = workspace:FindFirstChild("ButtonCompetition")
    if buttonCompetitionModel then
        local buttonsFolder = buttonCompetitionModel:WaitForChild("Build"):WaitForChild("Buttons")
        local clickDetectorFound = false
        for _, button in pairs(buttonsFolder:GetChildren()) do
            if button:IsA("Model") and button:FindFirstChild("Button") then
                local clickDetector = button.Button:FindFirstChildOfClass("ClickDetector")
                if clickDetector and not firedClickDetectors[clickDetector] then
                    fireclickdetector(clickDetector)
                    firedClickDetectors[clickDetector] = true
                    clickDetectorFound = true
                end
            end
        end

        if not clickDetectorFound then
            shouldContinueChecking = false
        end
    else
        shouldContinueChecking = false
    end
end

local clickAllButtons = buttonsTab:CreateButton({
    Name = "Click All Buttons",
    Callback = function()
        while shouldContinueChecking do
            checkAndFireClickDetectors()
            wait(0.1)
        end
    end
})

local cardboardRoomTab = window:CreateTab("Cardboard Room", 4483362458)
local scriptSectionCardboardRoom = cardboardRoomTab:CreateSection("Script")
local openAllDoorsButton = cardboardRoomTab:CreateButton({
    Name = "Open All Doors",
    Callback = function()
        local cardboardRoomModel = game.Workspace:FindFirstChild("CardboardRoom")
        if cardboardRoomModel then
            local buildModel = cardboardRoomModel:FindFirstChild("Build")
            if buildModel then
                local doorsFolder = buildModel:FindFirstChild("Doors")
                if doorsFolder then
                    for _, doorModel in pairs(doorsFolder:GetChildren()) do
                        if doorModel:IsA("Model") and doorModel.Name == "Door" then
                            local clickDetector = doorModel:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                fireclickdetector(clickDetector)
                            end
                        end
                    end
                end
            end
        end
    end
})

local forestCampTab = window:CreateTab("Forest Camp", 4483362458)
local scriptSectionForestCamp = forestCampTab:CreateSection("Script")
local removeUFOsButton = forestCampTab:CreateButton({
    Name = "Remove UFOs",
    Callback = function()
        local forestTwoStudCampModel = game.Workspace:FindFirstChild("Forest_TwoStudCamp")
        if forestTwoStudCampModel then
            local buildModel = forestTwoStudCampModel:FindFirstChild("Build")
            if buildModel then
                local ufosFolder = buildModel:FindFirstChild("UFOs")
                if ufosFolder then
                    ufosFolder:Destroy()
                end
                local ufoMoveTosFolder = buildModel:FindFirstChild("UFO_MoveTos")
                if ufoMoveTosFolder then
                    ufoMoveTosFolder:Destroy()
                end
            end
        end
    end
})

local autoPickupFirewoodButton = forestCampTab:CreateButton({
    Name = "Auto Pickup Firewood",
    Callback = function()
        local function onProximityPromptTriggered(proximityPrompt)
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            proximityPrompt.HoldDuration = 0
            proximityPrompt:InputHoldBegin()
            wait()
            proximityPrompt:InputHoldEnd()
        end

        local function checkProximityPrompt()
            local forestTwoStudCampModel = game.Workspace:FindFirstChild("Forest_TwoStudCamp")
            if forestTwoStudCampModel then
                local buildModel = forestTwoStudCampModel:FindFirstChild("Build")
                if buildModel then
                    local firewoodFolder = buildModel:FindFirstChild("Firewood")
                    if firewoodFolder then
                        local firewood = firewoodFolder:FindFirstChild("Firewood")
                        if firewood and firewood:IsA("UnionOperation") then
                            local proximityPrompt = firewood:FindFirstChildOfClass("ProximityPrompt")
                            if proximityPrompt and proximityPrompt:IsDescendantOf(workspace.CurrentCamera) then
                                onProximityPromptTriggered(proximityPrompt)
                            end
                        end
                    end
                end
            end
        end

        while wait(0.1) do
            checkProximityPrompt()
        end
    end
})

