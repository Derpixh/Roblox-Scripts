-- Create a tab named "JAOBA"
local jaobaTab = window:CreateTab("JAOBA", 4483362458)

-- Create a section in the JAOBA tab named "Script"
local scriptSectionJaoba = jaobaTab:CreateSection("Script")

-- Create a button in the JAOBA tab named "Find Jaoba"
local findJaobaButton = jaobaTab:CreateButton({
    Name = "Find Jaoba",
    Callback = function()
        local uesModel = game.Workspace:FindFirstChild("UES")
        if uesModel then
            local buildModel = uesModel:FindFirstChild("Build")
            if buildModel then
                local jaobaModel = buildModel:FindFirstChild("JAOBA")
                if jaobaModel then
                    local cardboardBox = jaobaModel:FindFirstChild("cardboard_box")
                    if cardboardBox then
                        local clickDetector = cardboardBox:FindFirstChildOfClass("ClickDetector")
                        if clickDetector then
                            clickDetector:FireClick()
                        end
                    end
                end
            end
        end
    end
})

-- Create a tab named "Giggler"
local gigglerTab = window:CreateTab("Giggler", 4483362458)

-- Create a section in the Giggler tab named "Script"
local scriptSectionGiggler = gigglerTab:CreateSection("Script")

-- Create a button in the Giggler tab named "Find Giggler"
local findGigglerButton = gigglerTab:CreateButton({
    Name = "Find Giggler",
    Callback = function()
        local mazeModel = game.Workspace:FindFirstChild("RandomMazeWIndows")
        if mazeModel then
            local buildModel = mazeModel:FindFirstChild("Build")
            if buildModel then
                local part = buildModel:FindFirstChild("Part")
                if part then
                    local touchInterest = part:FindFirstChildOfClass("TouchTransmitter")
                    if touchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part, 1)
                    end
                end
            end
        end
    end
})

-- Load saved configuration
Rayfield:LoadConfiguration()
