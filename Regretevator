-- Create a button in the Wipeout tab named "Instant Win"
local instantWinWipeoutButton = wipeoutTab:CreateButton({
    Name = "Instant Win",
    Callback = function()
        local model = game.Workspace:FindFirstChild("Splitsville_Wipeout")
        if model then
            local checkpointsFolder = model:FindFirstChild("Checkpoints")
            if checkpointsFolder then
                local endCheckpoint = checkpointsFolder:FindFirstChild("EndCheckpoint")
                if endCheckpoint then
                    local touchInterest = endCheckpoint:FindFirstChildOfClass("TouchTransmitter")
                    if touchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endCheckpoint, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endCheckpoint, 1)
                    end
                end
            end
        end
    end
})

