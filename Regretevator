local scriptSectionMines = minefieldTab:CreateSection("Mines")

local removeAllMinesButton = minefieldTab:CreateButton({
    Name = "Remove All Mines",
    SectionParent = scriptSectionMines,
    Callback = function()
        local minefieldModel = game.Workspace:FindFirstChild("Minefield")
        if minefieldModel then
            local buildModel = minefieldModel:FindFirstChild("Build")
            if buildModel then
                local minesFolder = buildModel:FindFirstChild("MinesFolder")
                if minesFolder then
                    minesFolder:Destroy()
                end
                
                local roombasFolder = buildModel:FindFirstChild("RoombasFolder")
                if roombasFolder then
                    roombasFolder:Destroy()
                end
            end
        end
    end
})

local cardboardRoomTab = window:CreateTab("Cardboard Room", 4483362458)
local scriptSectionCardboardRoom = cardboardRoomTab:CreateSection("Script")

local openAllDoorsButton = cardboardRoomTab:CreateButton({
    Name = "Open All Doors",
    Callback = function()
        local cardboardRoomModel = game.Workspace:FindFirstChild("CardboardRoom")
        if cardboardRoomModel then
            local buildModel = cardboardRoomModel:FindFirstChild("Build")
            if buildModel then
                local doorsFolder = buildModel:FindFirstChild("Doors")
                if doorsFolder then
                    for _, doorModel in pairs(doorsFolder:GetChildren()) do
                        if doorModel:IsA("Model") and doorModel.Name == "Door" then
                            local clickDetector = doorModel:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                fireclickdetector(clickDetector)
                            end
                        end
                    end
                end
            end
        end
    end
})

local forestCampTab = window:CreateTab("Forest Camp", 4483362458)
local scriptSectionForestCamp = forestCampTab:CreateSection("Script")

local removeUFOsButton = forestCampTab:CreateButton({
    Name = "Remove UFOs",
    Callback = function()
        local forestTwoStudCampModel = game.Workspace:FindFirstChild("Forest_TwoStudCamp")
        if forestTwoStudCampModel then
            local buildModel = forestTwoStudCampModel:FindFirstChild("Build")
            if buildModel then
                local ufosFolder = buildModel:FindFirstChild("UFOs")
                if ufosFolder then
                    ufosFolder:Destroy()
                end
                local ufoMoveTosFolder = buildModel:FindFirstChild("UFO_MoveTos")
                if ufoMoveTosFolder then
                    ufoMoveTosFolder:Destroy()
                end
            end
        end
    end
})

local scriptSectionFirewood = forestCampTab:CreateSection("Firewood")

local autoCollectFirewoodButton = forestCampTab:CreateButton({
    Name = "Auto Collect Firewood",
    SectionParent = scriptSectionFirewood,
    Callback = function()
        local firewoodFolder = workspace:FindFirstChild("ForestCamp") and workspace.ForestCamp:FindFirstChild("Build") and workspace.ForestCamp.Build:FindFirstChild("Firewood")
        if firewoodFolder then
            while true do
                for _, firewood in pairs(firewoodFolder:GetChildren()) do
                    if firewood.Name == "Firewood" then
                        local proximityPrompt = firewood:FindFirstChild("ProximityPrompt")
                        if proximityPrompt and proximityPrompt.Enabled and proximityPrompt.Root:IsDescendantOf(game.Players.LocalPlayer.Character) then
                            proximityPrompt.HoldDuration = 0
                            proximityPrompt:InputHoldBegin(game.Players.LocalPlayer)
                            wait()
                            proximityPrompt:InputHoldEnd(game.Players.LocalPlayer)
                        end
                    end
                end
                wait(0.1)
            end
        end
    end
})

