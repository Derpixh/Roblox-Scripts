local infectedRacingTab = window:CreateTab("Infected Racing", 4483362458)

local scriptSectionCheckpoints = infectedRacingTab:CreateSection("Checkpoints")

local completeCheckpointsButton = infectedRacingTab:CreateButton({
    Name = "Complete Checkpoints",
    SectionParent = scriptSectionCheckpoints,
    Callback = function()
        local infectedRacingModel = workspace:FindFirstChild("InfectedRacing")
        if infectedRacingModel then
            local buildModel = infectedRacingModel:FindFirstChild("Build")
            if buildModel then
                local checkpointsFolder = buildModel:FindFirstChild("Checkpoints")
                if checkpointsFolder then
                    for _, checkpointModel in pairs(checkpointsFolder:GetChildren()) do
                        if checkpointModel:IsA("Model") and checkpointModel.Name == "Checkpoint" then
                            local hitDetectPart = checkpointModel:FindFirstChild("HitDetect")
                            if hitDetectPart then
                                local touchInterest = hitDetectPart:FindFirstChild("TouchInterest")
                                if touchInterest then
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, hitDetectPart, 0)
                                    wait()
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, hitDetectPart, 1)
                                end
                            end
                        end
                    end
                end

                local winPart = buildModel:FindFirstChild("WINTHEGAME")
                if winPart then
                    local winTouchInterest = winPart:FindFirstChild("TouchInterest")
                    if winTouchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winPart, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winPart, 1)
                    end
                end
            end
        end
    end
})

local scriptSectionObstacles = infectedRacingTab:CreateSection("Obstacles")

local removeObstaclesButton = infectedRacingTab:CreateButton({
    Name = "Remove Obstacles",
    SectionParent = scriptSectionObstacles,
    Callback = function()
        local obstaclesFolder = workspace:FindFirstChild("InfectedRacing") and workspace.InfectedRacing:FindFirstChild("Build") and workspace.InfectedRacing.Build:FindFirstChild("Obstacles")
        if obstaclesFolder then
            for _, obstacleModel in pairs(obstaclesFolder:GetChildren()) do
                if obstacleModel:IsA("Model") then
                    local modelName = obstacleModel.Name:lower()
                    if modelName:find("bump") or modelName:find("bumpy") or modelName:find("wall") or modelName:find("crousher") or modelName:find("fafa") or modelName:find("pepperoni") or modelName:find("choose") then
                        obstacleModel:Destroy()
                    end
                end
            end
        end
    end
})

