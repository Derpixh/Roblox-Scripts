local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local window = Rayfield:CreateWindow({
    Name = "Regretevator",
    LoadingTitle = "By Derpixh",
    LoadingSubtitle = "Loading Assets...",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false,
        Invite = "sirius",
        RememberJoins = true
    }
})

local floodTab = window:CreateTab("Flood", 4483362458)

local scriptSectionFlood = floodTab:CreateSection("Script")

local instantWinButton = floodTab:CreateButton({
    Name = "Instant Win",
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        local touchInterest = bubblePart:FindFirstChildOfClass("TouchTransmitter")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, bubblePart, 0)
                            wait()
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, bubblePart, 1)
                        end
                    end
                end
            end
        end
    end
})

local teleportToFinishButton = floodTab:CreateButton({
    Name = "Teleport To Finish",
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = bubblePart.CFrame
                    end
                end
            end
        end
    end
})

local mazeTab = window:CreateTab("Maze", 4483362458)

local scriptSectionMaze = mazeTab:CreateSection("Script")

local collectPagesButton = mazeTab:CreateButton({
    Name = "Collect Pages",
    Callback = function()
        local funnyMaze = game.Workspace:FindFirstChild("FunnyMaze")
        if funnyMaze then
            local build = funnyMaze:FindFirstChild("Build")
            if build then
                local finalNotes = build:FindFirstChild("FinalNotes")
                if finalNotes then
                    for _, part in pairs(finalNotes:GetChildren()) do
                        if part:IsA("Part") and part.Name == "Note" then
                            local clickDetector = part:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                clickDetector.MouseClick:Fire()
                            end
                        end
                    end
                end
            end
        end
    end
})

local obbyTab = window:CreateTab("Obby", 4483362458)

local scriptSectionObby = obbyTab:CreateSection("Script")

local instantWinObbyButton = obbyTab:CreateButton({
    Name = "Instant Win Obby",
    Callback = function()
        local obbyModel = game.Workspace:FindFirstChild("Obby")
        if obbyModel then
            local buildModel = obbyModel:FindFirstChild("Build")
            if buildModel then
                local endPart = buildModel:FindFirstChild("EndPart")
                if endPart then
                    local touchInterest = endPart:FindFirstChildOfClass("TouchTransmitter")
                    if touchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endPart, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endPart, 1)
                    end
                end
            end
        end
    end
})

local teleportToEndButton = obbyTab:CreateButton({
    Name = "Teleport To End (USE AFTER INSTANT WIN)",
    Callback = function()
        local obbyModel = game.Workspace:FindFirstChild("Obby")
        if obbyModel then
            local buildModel = obbyModel:FindFirstChild("Build")
            if buildModel then
                local endPart = buildModel:FindFirstChild("EndPart")
                if endPart then
                    local part = endPart:FindFirstChild("Part")
                    if part then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
                    end
                end
            end
        end
    end
})

