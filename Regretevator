local gasa4Tab = window:CreateTab("GASA4", 4483362458)

local scriptSectionGASA4 = gasa4Tab:CreateSection("Steal & Extract")

local autoStealExtractButton = gasa4Tab:CreateButton({
    Name = "Auto Steal & Extract",
    SectionParent = scriptSectionGASA4,
    Callback = function()
        while workspace:FindFirstChild("GASA4") and workspace.GASA4:FindFirstChild("Build") and workspace.GASA4.Build:FindFirstChild("Stealables") do
            local stealablesFolder = workspace.GASA4.Build.Stealables
            local extractionPoint = workspace.GASA4.Build:FindFirstChild("ExtractionPoint")
            
            local nearestPart, nearestPrompt, shortestDistance = nil, nil, math.huge
            for _, part in pairs(stealablesFolder:GetChildren()) do
                if part:IsA("Part") then
                    local proximityPrompt = part:FindFirstChild("ThingButton")
                    if proximityPrompt and proximityPrompt:IsA("ProximityPrompt") and proximityPrompt.Enabled then
                        proximityPrompt.HoldDuration = 0
                        local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - part.Position).Magnitude
                        if distance < shortestDistance then
                            nearestPart, nearestPrompt, shortestDistance = part, proximityPrompt, distance
                        end
                    end
                end
            end
            
            if nearestPart and nearestPrompt then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = nearestPart.CFrame
                wait(0.6)

                while nearestPrompt.Enabled do
                    nearestPrompt:InputHoldBegin(game.Players.LocalPlayer)
                    wait()
                    nearestPrompt:InputHoldEnd(game.Players.LocalPlayer)
                end

                wait(0.6)

                if extractionPoint then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = extractionPoint.CFrame
                    wait(0.6)
                end
            else
                break
            end
        end
    end
})

local jermpopFactoryTab = window:CreateTab("Jermpop Factory", 4483362458)
local scriptSectionJermpopFactory = jermpopFactoryTab:CreateSection("Script")

local autoPressButtonsButton = jermpopFactoryTab:CreateButton({
    Name = "Auto Press Buttons",
    SectionParent = scriptSectionJermpopFactory,
    Callback = function()
        local cleanupButtonsFolder = workspace:FindFirstChild("JermpopFactory") and workspace.JermpopFactory:FindFirstChild("Build") and workspace.JermpopFactory.Build:FindFirstChild("CleanupButtons")
        if cleanupButtonsFolder then
            while true do
                local nearestPrim, nearestPrompt, shortestDistance = nil, nil, math.huge
                
                for _, buttonUnit in pairs(cleanupButtonsFolder:GetChildren()) do
                    if buttonUnit:IsA("Model") and buttonUnit.Name == "ButtonUnit" then
                        local primPart = buttonUnit:FindFirstChild("Prim")
                        if primPart then
                            local proximityPrompt = primPart:FindFirstChild("Prompt")
                            if proximityPrompt and proximityPrompt:IsA("ProximityPrompt") and proximityPrompt.Enabled then
                                local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - primPart.Position).Magnitude
                                if distance < shortestDistance then
                                    nearestPrim, nearestPrompt, shortestDistance = primPart, proximityPrompt, distance
                                end
                            end
                        end
                    end
                end

                if nearestPrim and nearestPrompt then
                    local targetPosition = nearestPrim.Position + nearestPrim.CFrame.LookVector * 3
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
                    wait(0.6)

                    while nearestPrompt.Enabled do
                        nearestPrompt:InputHoldBegin(game.Players.LocalPlayer)
                        wait()
                        nearestPrompt:InputHoldEnd(game.Players.LocalPlayer)
                    end
                else
                    break
                end

                wait(0.1)
            end
        end
    end
})

local theRockTab = window:CreateTab("THE ROCK", 4483362458)

local scriptSectionButtons = theRockTab:CreateSection("Buttons")

local autoPressButtons = theRockTab:CreateButton({
    Name = "Auto Press Buttons",
    SectionParent = scriptSectionButtons,
    Callback = function()
        while true do
            local section1Folder = workspace:FindFirstChild("THEROCK") and workspace.THEROCK:FindFirstChild("Build") and workspace.THEROCK.Build:FindFirstChild("Buttons") and workspace.THEROCK.Build.Buttons:FindFirstChild("Section1")
            if section1Folder then
                for _, buttonModel in pairs(section1Folder:GetChildren()) do
                    if buttonModel:IsA("Model") and buttonModel.Name == "Button" then
                        local collisionPart = buttonModel:FindFirstChild("Collision")
                        if collisionPart then
                            local touchInterest = collisionPart:FindFirstChild("TouchInterest")
                            if touchInterest then
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, collisionPart, 0)
                                wait()
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, collisionPart, 1)
                            end
                        end
                    end
                end
            end

            local section2Folder = workspace:FindFirstChild("THEROCK") and workspace.THEROCK:FindFirstChild("Build") and workspace.THEROCK.Build:FindFirstChild("Buttons") and workspace.THEROCK.Build.Buttons:FindFirstChild("Section2")
            if section2Folder then
                for _, buttonModel in pairs(section2Folder:GetChildren()) do
                    if buttonModel:IsA("Model") and buttonModel.Name == "Button" then
                        local collisionPart = buttonModel:FindFirstChild("Collision")
                        if collisionPart then
                            local touchInterest = collisionPart:FindFirstChild("TouchInterest")
                            if touchInterest then
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, collisionPart, 0)
                                wait()
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, collisionPart, 1)
                            end
                        end
                    end
                end
            end

            local section3Folder = workspace:FindFirstChild("THEROCK") and workspace.THEROCK:FindFirstChild("Build") and workspace.THEROCK.Build:FindFirstChild("Buttons") and workspace.THEROCK.Build.Buttons:FindFirstChild("Section3")
            if section3Folder then
                for _, buttonModel in pairs(section3Folder:GetChildren()) do
                    if buttonModel:IsA("Model") and buttonModel.Name == "Button" then
                        local collisionPart = buttonModel:FindFirstChild("Collision")
                        if collisionPart then
                            local touchInterest = collisionPart:FindFirstChild("TouchInterest")
                            if touchInterest then
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, collisionPart, 0)
                                wait()
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, collisionPart, 1)
                            end
                        end
                    end
                end
            end

            local winPart = workspace:FindFirstChild("THEROCK") and workspace.THEROCK:FindFirstChild("WinPart")
            if winPart then
                local winTouchInterest = winPart:FindFirstChild("TouchInterest")
                if winTouchInterest then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winPart, 0)
                    wait()
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winPart, 1)
                end
            end

            wait(0.1)
        end
    end
})

