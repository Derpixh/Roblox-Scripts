local lasersSection = statTab:CreateSection("Lasers")

local autoRemoveCapacitorLasersButton = statTab:CreateButton({
    Name = "Auto Remove Capacitor Lasers",
    SectionParent = lasersSection,
    Callback = function()
        while workspace:FindFirstChild("StatsDomain") and workspace.StatsDomain:FindFirstChild("Build") and workspace.StatsDomain.Build:FindFirstChild("ActiveUnits") do
            local activeUnits = workspace.StatsDomain.Build.ActiveUnits
            for _, capacitor in pairs(activeUnits:GetChildren()) do
                if capacitor:IsA("Model") and capacitor.Name == "Capacitor" then
                    local topLaser = capacitor:FindFirstChild("TopLaser")
                    if topLaser then
                        topLaser:Destroy()
                    end

                    local bottomLaser = capacitor:FindFirstChild("BottomLaser")
                    if bottomLaser then
                        bottomLaser:Destroy()
                    end
                end
            end
            wait()
        end
    end
})

local follyFightTab = window:CreateTab("Folly Fight", 4483362458)
local attacksSection = follyFightTab:CreateSection("Attacks")

local autoRemoveAttacksButton = follyFightTab:CreateButton({
    Name = "Auto Remove Attacks (Folly's, Lightning)",
    SectionParent = attacksSection,
    Callback = function()
        while workspace:FindFirstChild("FollyFight") and workspace.FollyFight:FindFirstChild("Build") do
            local buildModel = workspace.FollyFight.Build

            for _, attackModel in pairs(buildModel:GetChildren()) do
                if attackModel:IsA("Model") and attackModel.Name == "Folly" then
                    if attackModel:FindFirstChild("trail_claw_left") or attackModel:FindFirstChild("trail_claw_right") then
                        attackModel:Destroy()
                    end
                elseif attackModel:IsA("Model") and attackModel.Name == "Lightning" then
                    attackModel:Destroy()
                end
            end
            
            wait()
        end
    end
})

local gmFlatgrassTab = window:CreateTab("gm_flatgrass", 4483362458)
local coinsSection = gmFlatgrassTab:CreateSection("Coins")

local collectAllCoinsButton = gmFlatgrassTab:CreateButton({
    Name = "Collect All Coins (Teleport Method)",
    SectionParent = coinsSection,
    Callback = function()
        while workspace:FindFirstChild("gm_flatgrass") and workspace.gm_flatgrass:FindFirstChild("Coins") do
            local coinsFolder = workspace.gm_flatgrass.Coins
            local closestCoin, closestDistance = nil, math.huge

            for _, coin in pairs(coinsFolder:GetChildren()) do
                if coin:IsA("Part") and coin.Name == "Coin" then
                    local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - coin.Position).Magnitude
                    if distance < closestDistance then
                        closestCoin = coin
                        closestDistance = distance
                    end
                end
            end

            if closestCoin then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = closestCoin.CFrame
                wait()
            else
                break
            end
        end
    end
})

