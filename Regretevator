local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local window = Rayfield:CreateWindow({
    Name = "Regretevator - Alpha Hax",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Luxx",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false,
        Invite = "sirius",
        RememberJoins = true
    },
    KeySystem = true,
    KeySettings = {
        Title = "Luxx's Key System",
        Subtitle = "Enter Your Key",
        Note = "Rate The Features",
        FileName = "Sirius",
        SaveKey = false,
        Key = "2025"
    }
})

local floodTab = window:CreateTab("Flood", 4483362458)
local scriptSectionFlood = floodTab:CreateSection("Script")
local instantWinButton = floodTab:CreateButton({
    Name = "Instant Win",
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        local touchInterest = bubblePart:FindFirstChildOfClass("TouchTransmitter")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, bubblePart, 0)
                            wait()
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, bubblePart, 1)
                        end
                    end
                end
            end
        end
    end
})

local teleportToFinishButton = floodTab:CreateButton({
    Name = "Teleport To Finish",
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = bubblePart.CFrame
                    end
                end
            end
        end
    end
})

local mazeTab = window:CreateTab("Maze", 4483362458)
local scriptSectionMaze = mazeTab:CreateSection("Script")
local collectPagesButton = mazeTab:CreateButton({
    Name = "Collect Pages",
    Callback = function()
        local funnyMaze = workspace:FindFirstChild("FunnyMaze")
        if funnyMaze then
            local buildModel = funnyMaze:FindFirstChild("Build")
            if buildModel then
                local finalNotesFolder = buildModel:FindFirstChild("FinalNotes")
                if finalNotesFolder then
                    for _, notePart in pairs(finalNotesFolder:GetChildren()) do
                        if notePart:IsA("Part") and notePart.Name == "Note" then
                            local clickDetector = notePart:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                fireclickdetector(clickDetector)
                            end
                        end
                    end
                end
            end
        end
    end
})

