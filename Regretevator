                            local hitDetectPart = checkpointModel:FindFirstChild("HitDetect")
                            if hitDetectPart then
                                local touchInterest = hitDetectPart:FindFirstChild("TouchInterest")
                                if touchInterest then
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, hitDetectPart, 0)
                                    wait()
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, hitDetectPart, 1)
                                end
                            end
                        end
                    end
                end

                local winPart = buildModel:FindFirstChild("WINTHEGAME")
                if winPart then
                    local winTouchInterest = winPart:FindFirstChild("TouchInterest")
                    if winTouchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winPart, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winPart, 1)
                    end
                end
            end
        end
    end
