local scriptSectionGASA4 = gasa4Tab:CreateSection("Steal & Extract")

local autoStealExtractButton = gasa4Tab:CreateButton({
    Name = "Auto Steal & Extract",
    SectionParent = scriptSectionGASA4,
    Callback = function()
        local stealablesFolder = workspace:FindFirstChild("GASA4") and workspace.GASA4:FindFirstChild("Build") and workspace.GASA4.Build:FindFirstChild("Stealables")
        if stealablesFolder then
            for _, stealablePart in pairs(stealablesFolder:GetChildren()) do
                if stealablePart:IsA("Part") then
                    local proximityPrompt = stealablePart:FindFirstChild("ProximityPrompt")
                    if proximityPrompt and proximityPrompt.Enabled then
                        proximityPrompt.HoldDuration = 0

                        proximityPrompt:InputHoldBegin(game.Players.LocalPlayer)
                        wait()
                        proximityPrompt:InputHoldEnd(game.Players.LocalPlayer)

                        repeat wait(0.1) until not proximityPrompt.Enabled

                        wait(0.7)

                        local extractionPoint = workspace:FindFirstChild("GASA4") and workspace.GASA4:FindFirstChild("Build") and workspace.GASA4.Build:FindFirstChild("ExtractionPoint")
                        if extractionPoint then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = extractionPoint.CFrame
                            wait(0.7)
                        end
                    end
                end
            end
        end
    end
})

local jermpopFactoryTab = window:CreateTab("Jermpop Factory", 4483362458)

local scriptSectionJermpopFactory = jermpopFactoryTab:CreateSection("Script")

local autoPressButtonsButton = jermpopFactoryTab:CreateButton({
    Name = "Auto Press Buttons",
    SectionParent = scriptSectionJermpopFactory,
    Callback = function()
        local cleanupButtonsFolder = workspace:FindFirstChild("JermpopFactory") and workspace.JermpopFactory:FindFirstChild("Build") and workspace.JermpopFactory.Build:FindFirstChild("CleanupButtons")
        if cleanupButtonsFolder then
            while true do
                local nearestPrompt, nearestDistance = nil, math.huge
                for _, buttonUnit in pairs(cleanupButtonsFolder:GetChildren()) do
                    if buttonUnit:IsA("Model") and buttonUnit.Name == "ButtonUnit" then
                        local primPart = buttonUnit:FindFirstChild("Prim")
                        if primPart then
                            local proximityPrompt = primPart:FindFirstChild("Prompt")
                            if proximityPrompt and proximityPrompt:IsA("ProximityPrompt") and proximityPrompt.Enabled then
                                local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - primPart.Position).Magnitude
                                if distance < nearestDistance then
                                    nearestPrompt, nearestDistance = proximityPrompt, distance
                                end
                            end
                        end
                    end
                end
                
                if nearestPrompt then
                    nearestPrompt.HoldDuration = 0
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = nearestPrompt.Parent.CFrame
                    wait(0.7)
                    
                    nearestPrompt:InputHoldBegin(game.Players.LocalPlayer)
                    wait()
                    nearestPrompt:InputHoldEnd(game.Players.LocalPlayer)
                    
                    wait(0.7)
                else
                    break
                end
            end
        end
    end
})

