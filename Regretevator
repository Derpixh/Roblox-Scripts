local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local window = Rayfield:CreateWindow({
    Name = "Regretevator - Alpha Hax",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Luxx",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false,
        Invite = "sirius",
        RememberJoins = true
    },
    KeySystem = true,
    KeySettings = {
        Title = "Luxx's Key System",
        Subtitle = "Enter Your Key",
        Note = "Rate The Features",
        FileName = "Sirius",
        SaveKey = false,
        Key = "2025"
    }
})

local elevatorTab = window:CreateTab("Elevator", 4483362458)
local railsSection = elevatorTab:CreateSection("Rails")

local invincibilityRailsButton = elevatorTab:CreateButton({
    Name = "Invincibility To Burning Rails",
    SectionParent = railsSection,
    Callback = function()
        while workspace:FindFirstChild("Elevator") and workspace.Elevator:FindFirstChild("CollisionFixers") do
            local collisionFixers = workspace.Elevator.CollisionFixers
            
            for _, part in pairs(collisionFixers:GetChildren()) do
                if part:IsA("Part") and part:FindFirstChild("TouchInterest") then
                    part.TouchInterest:Destroy()
                end
            end
            
            wait(0.02)
        end
    end
})

local spudSection = elevatorTab:CreateSection("Spud")

local autoClickSpudButton = elevatorTab:CreateButton({
    Name = "Auto Click Spud (HE WILL EXPLODE)",
    SectionParent = spudSection,
    Callback = function()
        while workspace:FindFirstChild("Elevator") and workspace.Elevator:FindFirstChild("Npc") and workspace.Elevator.Npc:FindFirstChild("Npcs") and workspace.Elevator.Npc.Npcs:FindFirstChild("Spud!") do
            local spudClickDetector = workspace.Elevator.Npc.Npcs["Spud!"]:FindFirstChild("ClickDetector")
            
            if spudClickDetector then
                fireclickdetector(spudClickDetector)
            end
            
            wait()
        end
    end
})

local floodFillMinesTab = window:CreateTab("Flood Fill Mines", 4483362458)
local winSection = floodFillMinesTab:CreateSection("Win")

local instantWinButton = floodFillMinesTab:CreateButton({
    Name = "Instant Win",
    SectionParent = winSection,
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        local touchInterest = bubblePart:FindFirstChildOfClass("TouchTransmitter")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, bubblePart, 0)
                            wait()
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, bubblePart, 1)
                        end
                    end
                end
            end
        end
    end
})

local endSection = floodFillMinesTab:CreateSection("End")

local teleportToFinishButton = floodFillMinesTab:CreateButton({
    Name = "Teleport To Finish",
    SectionParent = endSection,
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = bubblePart.CFrame
                    end
                end
            end
        end
    end
})

local teleportToFinishButton = floodTab:CreateButton({
    Name = "Teleport To Finish",
    Callback = function()
        local floodFillMineModel = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMineModel then
            local buildModel = floodFillMineModel:FindFirstChild("Build")
            if buildModel then
                local shieldModel = buildModel:FindFirstChild("Shield")
                if shieldModel then
                    local bubblePart = shieldModel:FindFirstChild("Bubble")
                    if bubblePart then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = bubblePart.CFrame
                    end
                end
            end
        end
    end
})

local funnyMazeTab = window:CreateTab("Funny Maze", 4483362458)

local pagesSection = funnyMazeTab:CreateSection("Pages")

local collectPagesButton = funnyMazeTab:CreateButton({
    Name = "Collect Pages",
    SectionParent = pagesSection,
    Callback = function()
        local funnyMaze = workspace:FindFirstChild("FunnyMaze")
        if funnyMaze then
            local buildModel = funnyMaze:FindFirstChild("Build")
            if buildModel then
                local finalNotesFolder = buildModel:FindFirstChild("FinalNotes")
                if finalNotesFolder then
                    for _, notePart in pairs(finalNotesFolder:GetChildren()) do
                        if notePart:IsA("Part") and notePart.Name == "Note" then
                            local clickDetector = notePart:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                fireclickdetector(clickDetector)
                            end
                        end
                    end
                end
            end
        end
    end
})

Rayfield:LoadConfiguration()
