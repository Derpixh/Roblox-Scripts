local scriptSectionFindThePath = findThePathTab:CreateSection("ESP")

local espCorrectTilesButton = findThePathTab:CreateButton({
    Name = "ESP Correct Tiles",
    SectionParent = scriptSectionFindThePath,
    Callback = function()
        if workspace:FindFirstChild("FindThePath") and workspace.FindThePath:FindFirstChild("Build") and workspace.FindThePath.Build:FindFirstChild("Tiles") then
            local tiles = workspace.FindThePath.Build.Tiles

            for _, tileTemplate in pairs(tiles:GetChildren()) do
                if tileTemplate:IsA("Model") and tileTemplate.Name == "tile_template" then
                    local collPart = tileTemplate:FindFirstChild("coll")
                    if collPart and collPart:IsA("Part") and collPart.CanCollide then
                        local highlight = Instance.new("Highlight")
                        highlight.Parent = tileTemplate
                        highlight.FillColor = Color3.new(0, 1, 0)
                    end
                end
            end
        end
    end
})

