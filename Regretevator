        if findThePathModel then
            local buildModel = findThePathModel:FindFirstChild("Build")
            if buildModel then
                local endModel = buildModel:FindFirstChild("End")
                if endModel then
                    local winZonePart = endModel:FindFirstChild("win_zone")
                    if winZonePart then
                        local touchInterest = winZonePart:FindFirstChildOfClass("TouchTransmitter")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winZonePart, 0)
                            wait()
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, winZonePart, 1)
                        end
                    end
                end
            end
        end
    end
})

