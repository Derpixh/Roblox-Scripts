-- Bootstrapping Rayfield UI
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create a window
local window = Rayfield:CreateWindow({
    Name = "Regretevator",
    LoadingTitle = "By Derpixh",
    LoadingSubtitle = "Loading Assets...",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false,
        Invite = "sirius",
        RememberJoins = true
    }
})

-- Create a tab named "Flood"
local floodTab = window:CreateTab("Flood", 4483362458)

-- Create a section in the Flood tab named "Script"
local scriptSectionFlood = floodTab:CreateSection("Script")

-- Create a button in the Flood tab named "ESP Mines"
local espMinesButton = floodTab:CreateButton({
    Name = "ESP Mines",
    Callback = function()
        local floodFillMine = game.Workspace:FindFirstChild("FloodFillMine")
        if floodFillMine then
            local build = floodFillMine:FindFirstChild("Build")
            if build then
                for _, layerName in pairs({"Layer1", "Layer2", "Layer3"}) do
                    local layer = build:FindFirstChild(layerName)
                    if layer then
                        local layerHole = layer:FindFirstChild(layerName .. "Hole")
                        if layerHole then
                            local highlight = layerHole:FindFirstChildOfClass("Highlight")
                            if highlight then
                                highlight:Destroy()
                            else
                                highlight = Instance.new("Highlight")
                                highlight.Parent = layerHole
                                highlight.FillColor = Color3.fromRGB(255, 0, 0)
                            end
                        end
                    end
                end
            end
        end
    end
})

-- Create a tab named "Maze"
local mazeTab = window:CreateTab("Maze", 4483362458)

-- Create a section in the Maze tab named "Script"
local scriptSectionMaze = mazeTab:CreateSection("Script")

-- Create a button in the Maze tab named "Collect Pages"
local collectPagesButton = mazeTab:CreateButton({
    Name = "Collect Pages",
    Callback = function()
        local funnyMaze = game.Workspace:FindFirstChild("FunnyMaze")
        if funnyMaze then
            local build = funnyMaze:FindFirstChild("Build")
            if build then
                local finalNotes = build:FindFirstChild("FinalNotes")
                if finalNotes then
                    for _, part in pairs(finalNotes:GetChildren()) do
                        if part:IsA("Part") and part.Name == "Note" then
                            local clickDetector = part:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                clickDetector:FireClick()
                            end
                        end
                    end
                end
            end
        end
    end
})

-- Create a tab named "Obby"
local obbyTab = window:CreateTab("Obby", 4483362458)

-- Create a section in the Obby tab named "Script"
local scriptSectionObby = obbyTab:CreateSection("Script")

-- Create a button in the Obby tab named "Instant Win Obby"
local instantWinObbyButton = obbyTab:CreateButton({
    Name = "Instant Win Obby",
    Callback = function()
        local obbyModel = game.Workspace:FindFirstChild("Obby")
        if obbyModel then
            local buildModel = obbyModel:FindFirstChild("Build")
            if buildModel then
                local endPart = buildModel:FindFirstChild("EndPart")
                if endPart then
                    local touchInterest = endPart:FindFirstChildOfClass("TouchTransmitter")
                    if touchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endPart, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endPart, 1)
                    end
                end
            end
        end
    end
})

-- Create a button in the Obby tab named "Teleport To End (USE AFTER INSTANT WIN)"
local teleportToEndButton = obbyTab:CreateButton({
    Name = "Teleport To End (USE AFTER INSTANT WIN)",
    Callback = function()
        local obbyModel = game.Workspace:FindFirstChild("Obby")
        if obbyModel then
            local buildModel = obbyModel:FindFirstChild("Build")
            if buildModel then
                local endPart = buildModel:FindFirstChild("EndPart")
                if endPart then
                    local part = endPart:FindFirstChild("Part")
                    if part then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = endPart.CFrame
                    end
                end
            end
        end
    end
})

