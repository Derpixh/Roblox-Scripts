local surviveArea51Tab = window:CreateTab("Survive Area 51", 4483362458)

local scriptSectionSurviveArea51 = surviveArea51Tab:CreateSection("Script")

local function teleportToGenerator(roomName)
    local generatorPart = workspace:FindFirstChild("SurvivalTheArea51") and workspace.SurvivalTheArea51:FindFirstChild("Build") and workspace.SurvivalTheArea51.Build:FindFirstChild(roomName) and workspace.SurvivalTheArea51.Build[roomName]:FindFirstChild("Generator")
    if generatorPart then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = generatorPart.CFrame
        wait(0.5)

        local proximityPrompt = generatorPart:FindFirstChild("ProximityPrompt")
        if proximityPrompt then
            proximityPrompt.HoldDuration = 0
            proximityPrompt:InputHoldBegin(game.Players.LocalPlayer)
            wait()
            proximityPrompt:InputHoldEnd(game.Players.LocalPlayer)
        end
    end
end

local teleportToAngryWallGeneratorButton = surviveArea51Tab:CreateButton({
    Name = "Teleport to Angry Wall Generator",
    Callback = function()
        teleportToGenerator("AngryWallRoom")
    end
})

local teleportToDougGeneratorButton = surviveArea51Tab:CreateButton({
    Name = "Teleport to Doug Generator",
    Callback = function()
        teleportToGenerator("DougRoom")
    end
})

local teleportToJeremyGeneratorButton = surviveArea51Tab:CreateButton({
    Name = "Teleport To Jeremy Generator",
    Callback = function()
        teleportToGenerator("JeremyRoom")
    end
})

local teleportToKillerGeneratorButton = surviveArea51Tab:CreateButton({
    Name = "Teleport To Killer Generator",
    Callback = function()
        teleportToGenerator("KillerRoom")
    end
})

local teleportToEndGeneratorButton = surviveArea51Tab:CreateButton({
    Name = "Teleport To End Generator",
    Callback = function()
        teleportToGenerator("EndRoom")
    end
})

