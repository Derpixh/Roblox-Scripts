            for _, tntPart in pairs(tntFolder:GetChildren()) do
                if tntPart:IsA("Part") and tntPart.Name == "tnt" then
                    local touchInterest = tntPart:FindFirstChild("TouchInterest")
                    if touchInterest then
                        touchInterest:Destroy()
                    end
                end
            end
            
            wait()
        end
    end
})

