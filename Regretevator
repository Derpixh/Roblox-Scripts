local scriptSectionMines = minefieldTab:CreateSection("Mines")

local removeAllMinesButton = minefieldTab:CreateButton({
    Name = "Remove All Mines",
    SectionParent = scriptSectionMines,
    Callback = function()
        local minefieldModel = game.Workspace:FindFirstChild("Minefield")
        if minefieldModel then
            local buildModel = minefieldModel:FindFirstChild("Build")
            if buildModel then
                local minesFolder = buildModel:FindFirstChild("MinesFolder")
                if minesFolder then
                    minesFolder:Destroy()
                end
                
                local roombasFolder = buildModel:FindFirstChild("RoombasFolder")
                if roombasFolder then
                    roombasFolder:Destroy()
                end
            end
        end
    end
})

local endSection = minefieldTab:CreateSection("End")

local teleportToOtherSideButton = minefieldTab:CreateButton({
    Name = "Teleport To Other Side",
    SectionParent = endSection,
    Callback = function()
        local winPart = workspace:FindFirstChild("Minefield") and workspace.Minefield:FindFirstChild("Build") and workspace.Minefield.Build:FindFirstChild("WinPart")
        if winPart then
            local targetPosition = winPart.Position + winPart.CFrame.LookVector * 2
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
        end
    end
})

local cardboardRoomTab = window:CreateTab("Cardboard Room", 4483362458)
local scriptSectionCardboardRoom = cardboardRoomTab:CreateSection("Script")

local openAllDoorsButton = cardboardRoomTab:CreateButton({
    Name = "Open All Doors",
    Callback = function()
        local cardboardRoomModel = game.Workspace:FindFirstChild("CardboardRoom")
        if cardboardRoomModel then
            local buildModel = cardboardRoomModel:FindFirstChild("Build")
            if buildModel then
                local doorsFolder = buildModel:FindFirstChild("Doors")
                if doorsFolder then
                    for _, doorModel in pairs(doorsFolder:GetChildren()) do
                        if doorModel:IsA("Model") and doorModel.Name == "Door" then
                            local clickDetector = doorModel:FindFirstChildOfClass("ClickDetector")
                            if clickDetector then
                                fireclickdetector(clickDetector)
                            end
                        end
                    end
                end
            end
        end
    end
})

local forestCampTab = window:CreateTab("Forest Camp", 4483362458)
local scriptSectionForestCamp = forestCampTab:CreateSection("Script")

local removeUFOsButton = forestCampTab:CreateButton({
    Name = "Remove UFOs",
    Callback = function()
        local forestTwoStudCampModel = game.Workspace:FindFirstChild("Forest_TwoStudCamp")
        if forestTwoStudCampModel then
            local buildModel = forestTwoStudCampModel:FindFirstChild("Build")
            if buildModel then
                local ufosFolder = buildModel:FindFirstChild("UFOs")
                if ufosFolder then
                    ufosFolder:Destroy()
                end
                local ufoMoveTosFolder = buildModel:FindFirstChild("UFO_MoveTos")
                if ufoMoveTosFolder then
                    ufoMoveTosFolder:Destroy()
                end
            end
        end
    end
})

local scriptSectionFirewood = forestCampTab:CreateSection("Firewood")

local autoPickupPlaceFirewoodButton = forestCampTab:CreateButton({
    Name = "Auto Pickup/Place Firewood",
    SectionParent = scriptSectionFirewood,
    Callback = function()
        while workspace:FindFirstChild("Forest_TwoStudCamp") and workspace.Forest_TwoStudCamp:FindFirstChild("Build") and workspace.Forest_TwoStudCamp.Build:FindFirstChild("Firewood") do
            local firewoodFolder = workspace.Forest_TwoStudCamp.Build.Firewood
            local nearestPart, nearestPrompt, shortestDistance = nil, nil, math.huge
            
            for _, firewood in pairs(firewoodFolder:GetChildren()) do
                if firewood.Name == "Firewood" then
                    local proximityPrompt = firewood:FindFirstChild("ProximityPrompt")
                    if proximityPrompt and proximityPrompt.Enabled then
                        proximityPrompt.HoldDuration = 0
                        local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - firewood.Position).Magnitude
                        if distance < shortestDistance then
                            nearestPart, nearestPrompt, shortestDistance = firewood, proximityPrompt, distance
                        end
                    end
                end
            end
            
            if nearestPart and nearestPrompt then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = nearestPart.CFrame
                wait(0.6)

                while nearestPrompt.Enabled do
                    nearestPrompt:InputHoldBegin(game.Players.LocalPlayer)
                    wait()
                    nearestPrompt:InputHoldEnd(game.Players.LocalPlayer)
                end

                wait(0.6)

                local cauldronPromptPart = workspace.Forest_TwoStudCamp.Build:FindFirstChild("Cauldron") and workspace.Forest_TwoStudCamp.Build.Cauldron:FindFirstChild("PromptPart")
                if cauldronPromptPart then
                    local cauldronPrompt = cauldronPromptPart:FindFirstChild("ProximityPrompt")
                    if cauldronPrompt and cauldronPrompt.Enabled then
                        cauldronPrompt.HoldDuration = 0
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cauldronPromptPart.CFrame + cauldronPromptPart.CFrame.LookVector * 3
                        wait(0.6)

                        while cauldronPrompt.Enabled do
                            cauldronPrompt:InputHoldBegin(game.Players.LocalPlayer)
                            wait()
                            cauldronPrompt:InputHoldEnd(game.Players.LocalPlayer)
                        end

                        wait(0.6)
                    end
                end
            else
                break
            end
        end
    end
})

