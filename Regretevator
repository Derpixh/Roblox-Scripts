local scriptSectionRespawnCenter = superDropperTab:CreateSection("Respawn Center")

local respawnButton = superDropperTab:CreateButton({
    Name = "Respawn (You Can Do The Dropper Again)",
    SectionParent = scriptSectionRespawnCenter,
    Callback = function()
        local returnPortal = workspace:FindFirstChild("SuperDropper") and workspace.SuperDropper:FindFirstChild("RespawnCenter") and workspace.SuperDropper.RespawnCenter:FindFirstChild("ReturnPortal")
        if returnPortal then
            local touchInterest = returnPortal:FindFirstChild("TouchInterest")
            if touchInterest then
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, returnPortal, 0)
                wait()
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, returnPortal, 1)
            end
        end
    end
})

local automaticMethodSection = superDropperTab:CreateSection("Automatic Method")

local autoFarmCoinsButton = superDropperTab:CreateButton({
    Name = "Auto Farm Coins (Win + Respawn Method)",
    SectionParent = automaticMethodSection,
    Callback = function()
        while workspace:FindFirstChild("SuperDropper") do
            local winPool = workspace.SuperDropper:FindFirstChild("Build") and workspace.SuperDropper.Build:FindFirstChild("WinPool")
            if winPool then
                local touchTransmitter = winPool:FindFirstChildWhichIsA("TouchTransmitter")
                if touchTransmitter then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, touchTransmitter.Parent, 0)
                    wait()
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, touchTransmitter.Parent, 1)
                end
            end

            wait(0.3)

            local returnPortal = workspace.SuperDropper:FindFirstChild("RespawnCenter") and workspace.SuperDropper.RespawnCenter:FindFirstChild("ReturnPortal")
            if returnPortal then
                local touchTransmitter = returnPortal:FindFirstChildWhichIsA("TouchTransmitter")
                if touchTransmitter then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, touchTransmitter.Parent, 0)
                    wait()
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, touchTransmitter.Parent, 1)
                end
            end

            wait(0.1)
        end
    end
})

