local redballDinerTab = window:CreateTab("Redball Diner", 4483362458)
local reddySection = redballDinerTab:CreateSection("Reddy")

local autoPressResetPromptButton = redballDinerTab:CreateButton({
    Name = "Auto Press Reset Prompt",
    SectionParent = reddySection,
    Callback = function()
        local resetPrompt = workspace:FindFirstChild("RedballDiner")
            and workspace.RedballDiner:FindFirstChild("Build")
            and workspace.RedballDiner.Build:FindFirstChild("Animatronics")
            and workspace.RedballDiner.Build.Animatronics:FindFirstChild("Reddy")
            and workspace.RedballDiner.Build.Animatronics.Reddy:FindFirstChild("HumanoidRootPart")
            and workspace.RedballDiner.Build.Animatronics.Reddy.HumanoidRootPart:FindFirstChild("ResetPrompt")
        
        if resetPrompt then
            resetPrompt.Enabled = true
            resetPrompt.HoldDuration = 0
            
            local reddyPosition = workspace.RedballDiner.Build.Animatronics.Reddy.HumanoidRootPart.Position
            local forwardOffset = workspace.RedballDiner.Build.Animatronics.Reddy.HumanoidRootPart.CFrame.LookVector * 3
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(reddyPosition + forwardOffset)

            repeat
                local playerDistance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - resetPrompt.Parent.Position).Magnitude
                if playerDistance <= resetPrompt.MaxActivationDistance then
                    resetPrompt:InputHoldBegin()
                    wait()
                    resetPrompt:InputHoldEnd()
                end
            until not resetPrompt.Enabled
        end
    end
})

local birthdayTab = window:CreateTab("Birthday", 4483362458)

local scriptSectionDestructible = birthdayTab:CreateSection("Destructible")

local destructEverythingButton = birthdayTab:CreateButton({
    Name = "Destruct Everything",
    SectionParent = scriptSectionDestructible,
    Callback = function()
        local destructibleFolder = workspace:FindFirstChild("Birthday") and workspace.Birthday:FindFirstChild("Build") and workspace.Birthday.Build:FindFirstChild("destructible")
        if destructibleFolder then
            for _, destructibleModel in pairs(destructibleFolder:GetChildren()) do
                if destructibleModel:IsA("Model") then
                    local clickDetector = destructibleModel:FindFirstChildOfClass("ClickDetector")
                    if clickDetector then
                        fireclickdetector(clickDetector)
                    end
                end
            end
        end
    end
})

local redballTempleTab = window:CreateTab("Redball Temple", 4483362458)
local scriptSectionSpray = redballTempleTab:CreateSection("Spray")

local autoPickupSprayButton = redballTempleTab:CreateButton({
    Name = "Auto Pickup Spray",
    SectionParent = scriptSectionSpray,
    Callback = function()
        local claimPart = workspace:FindFirstChild("RedBallTemple") and workspace.RedBallTemple:FindFirstChild("Build") and workspace.RedBallTemple.Build:FindFirstChild("Claim")
        if claimPart then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = claimPart.CFrame
            wait(0.5)

            local proximityPrompt = claimPart:FindFirstChild("ItemStockProximityPrompt")
            if proximityPrompt then
                proximityPrompt.HoldDuration = 0
                if proximityPrompt.Enabled then
                    proximityPrompt:InputHoldBegin(game.Players.LocalPlayer)
                    wait()
                    proximityPrompt:InputHoldEnd(game.Players.LocalPlayer)
                end
            end

            wait(0.5)

            local metallicFloor = workspace:FindFirstChild("Elevator") and workspace.Elevator:FindFirstChild("MetallicFloor")
            if metallicFloor then
                local targetPosition = metallicFloor.Position + Vector3.new(0, 1, 0)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
            end
        end
    end
})

local infectionApartmentTab = window:CreateTab("Infection Apartment", 4483362458)

local scriptSectionPowerBoxes = infectionApartmentTab:CreateSection("Power Boxes")

local autoPowerBoxesButton = infectionApartmentTab:CreateButton({
    Name = "Auto Power-Boxes",
    SectionParent = scriptSectionPowerBoxes,
    Callback = function()
        local powerBoxesFolder = workspace:FindFirstChild("InfectionApartment") and workspace.InfectionApartment:FindFirstChild("Immune") and workspace.InfectionApartment.Immune:FindFirstChild("PowerBoxes")
        if powerBoxesFolder then
            for _, powerBoxModel in pairs(powerBoxesFolder:GetChildren()) do
                if powerBoxModel:IsA("Model") and powerBoxModel.Name == "PowerBox" then
                    local valvePart = powerBoxModel:FindFirstChild("Valve")
                    if valvePart then
                        local targetPosition = valvePart.Position + valvePart.CFrame.LookVector * 3
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
                        wait(0.6)

                        local valvePromptPart = valvePart:FindFirstChild("ValvePrompt")
                        if valvePromptPart then
                            local proximityPrompt = valvePromptPart:FindFirstChild("ProximityPrompt")
                            if proximityPrompt and proximityPrompt.Enabled then
                                proximityPrompt.HoldDuration = 0
                                proximityPrompt:InputHoldBegin(game.Players.LocalPlayer)
                                wait()
                                proximityPrompt:InputHoldEnd(game.Players.LocalPlayer)

                                repeat wait(0.1) until not proximityPrompt.Enabled
                            end
                        end
                    end
                end
            end
        end
    end
})

local pizzaTab = window:CreateTab("Pizza Delivery", 4483362458)
local pizzaSection = pizzaTab:CreateSection("Pizza")

pizzaTab:CreateButton({
    Name = "Auto Get/Deliver Pizzas",
    SectionParent = pizzaSection,
    Callback = function()
        local player = Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local rootPart = character:WaitForChild("HumanoidRootPart")

        local function safeTouch(part)
            firetouchinterest(rootPart, part, 0)
            task.wait(0.050)
            firetouchinterest(rootPart, part, 1)
        end

        local delivery = workspace:FindFirstChild("PizzaDelivery")
        if not delivery then return end

        local build = delivery:FindFirstChild("Build")
        if not build then return end

        local pizzaBoxes = build:FindFirstChild("PizzaBoxes")
        local pizzaDoors = build:FindFirstChild("PizzaDoors")

        if pizzaBoxes then
            local count = 0
            for _, meshPart in ipairs(pizzaBoxes:GetChildren()) do
                if meshPart:IsA("MeshPart") and meshPart.Name == "PizzaBox" and count < 10 then
                    local touchPart = meshPart.Parent
                    if touchPart and touchPart:IsA("Part") then
                        safeTouch(touchPart)
                        count += 1
                        task.wait(0.050)
                    end
                end
            end
        end

        if pizzaDoors then
            for _, door in ipairs(pizzaDoors:GetChildren()) do
                if door:IsA("Model") and door.Name:find("Door") then
                    local pad = door:FindFirstChild("DeliverPartPad")
                    if pad then
                        local gui = pad:FindFirstChild("SurfaceGui")
                        if gui then
                            for _, frame in ipairs(gui:GetChildren()) do
                                if frame:IsA("Frame") and frame.Name == "Frame" and frame.BackgroundColor3 == Color3.new(0, 1, 0) then
                                    local touchPart = door:FindFirstChild("Touch")
                                    if touchPart and touchPart:IsA("Part") then
                                        safeTouch(touchPart)
                                        task.wait(0.050)
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
})

local petCaptureTab = window:CreateTab("Pet Capture Deluxe", 4483362458)
local petsSection = petCaptureTab:CreateSection("Pets")

local autoCapturePetsButton = petCaptureTab:CreateButton({
    Name = "Auto Capture Pets",
    SectionParent = petsSection,
    Callback = function()
        local function getNearestEnabledPrompt()
            local activeMonsters = workspace:FindFirstChild("PetCaptureDeluxe") and workspace.PetCaptureDeluxe:FindFirstChild("Build") and workspace.PetCaptureDeluxe.Build:FindFirstChild("ActiveMonsters")
            local nearestPart, nearestPrompt, shortestDistance = nil, nil, math.huge

            if activeMonsters then
                for _, part in pairs(activeMonsters:GetChildren()) do
                    if part:IsA("Part") then
                        local prompt = part:FindFirstChildWhichIsA("ProximityPrompt")
                        if prompt and prompt.Enabled then
                            local distance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - part.Position).Magnitude
                            if distance < shortestDistance then
                                nearestPart, nearestPrompt, shortestDistance = part, prompt, distance
                            end
                        end
                    end
                end
            end

            return nearestPart, nearestPrompt
        end

        while true do
            local targetPart, targetPrompt = getNearestEnabledPrompt()
            if targetPart and targetPrompt then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPart.CFrame
                wait(0.7)
                
                while targetPrompt.Enabled do
                    targetPrompt:InputHoldBegin(game.Players.LocalPlayer)
                    wait()
                    targetPrompt:InputHoldEnd(game.Players.LocalPlayer)
                end
            else
                break
            end
            wait(0.1)
        end
    end
})

local slimYimTab = window:CreateTab("Slim Yim", 4483362458)
local attacksSection = slimYimTab:CreateSection("Attacks")

local autoRemoveAttacksButton = slimYimTab:CreateButton({
    Name = "Auto Remove Attacks",
    SectionParent = attacksSection,
    Callback = function()
        local attacks = workspace:FindFirstChild("SlimYim") and workspace.SlimYim:FindFirstChild("RoomScript") and workspace.SlimYim.RoomScript:FindFirstChild("Attacks")
        if attacks then
            attacks:Destroy()
        end

        while workspace:FindFirstChild("SlimYim") do
            local challengeFrank = workspace.SlimYim:FindFirstChild("ChallengeFrank")
            if challengeFrank then
                challengeFrank:Destroy()
            end

            local sillayPart = workspace.SlimYim:FindFirstChild("Sillay")
            if sillayPart then
                sillayPart:Destroy()
            end
            
            local faceModel = workspace.SlimYim:FindFirstChild("Face")
            if faceModel then
                faceModel:Destroy()
            end

            local dieModel = workspace.SlimYim:FindFirstChild("DIE")
            if dieModel then
                dieModel:Destroy()
            end

            for _, model in pairs(workspace.SlimYim:GetChildren()) do
                if model:IsA("Model") and model:FindFirstChild("Strips") then
                    model:Destroy()
                end
            end
            
            wait()
        end
    end
})

local orbsSection = slimYimTab:CreateSection("Orbs")

local autoKickOrbsButton = slimYimTab:CreateButton({
    Name = "Auto Kick Orbs (You Have To Walk To Them)",
    SectionParent = orbsSection,
    Callback = function()
        local slimYimBuild = workspace:FindFirstChild("SlimYim") and workspace.SlimYim:FindFirstChild("Build")
        if slimYimBuild then
            for _, orb in pairs(slimYimBuild:GetChildren()) do
                if orb:IsA("Model") and orb.Name == "Orb" then
                    local proximityPrompt = orb:FindFirstChild("ProximityPrompt")
                    if proximityPrompt then
                        local playerDistance = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - orb:GetPivot().Position).Magnitude
                        if playerDistance <= proximityPrompt.MaxActivationDistance then
                            proximityPrompt:InputHoldBegin()
                            wait()
                            proximityPrompt:InputHoldEnd()
                        end
                    end
                end
            end
        end
    end
})

