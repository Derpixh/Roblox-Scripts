local stanelyRoomTab = window:CreateTab("Stanely Room", 4483362458)

local scriptSectionStanelyRoom = stanelyRoomTab:CreateSection("Script")

local instantWinStanelyRoomButton = stanelyRoomTab:CreateButton({
    Name = "Instant Win",
    Callback = function()
        local stanelyRoomModel = game.Workspace:FindFirstChild("StanelyRoom")
        if stanelyRoomModel then
            local buildModel = stanelyRoomModel:FindFirstChild("Build")
            if buildModel then
                local generatedModel = buildModel:FindFirstChild("Generated")
                if generatedModel then
                    local endingModel = generatedModel:FindFirstChild("Ending")
                    if endingModel then
                        local endTouchPart = endingModel:FindFirstChild("EndTouch")
                        if endTouchPart then
                            local touchInterest = endTouchPart:FindFirstChildOfClass("TouchTransmitter")
                            if touchInterest then
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endTouchPart, 0)
                                wait()
                                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, endTouchPart, 1)
                            end
                        end
                    end
                end
            end
        end
    end
})

local teleportToEndStanelyRoomButton = stanelyRoomTab:CreateButton({
    Name = "Teleport To End",
    Callback = function()
        local stanelyRoomModel = game.Workspace:FindFirstChild("StanelyRoom")
        if stanelyRoomModel then
            local buildModel = stanelyRoomModel:FindFirstChild("Build")
            if buildModel then
                local generatedModel = buildModel:FindFirstChild("Generated")
                if generatedModel then
                    local endingModel = generatedModel:FindFirstChild("Ending")
                    if endingModel then
                        local endTouchPart = endingModel:FindFirstChild("EndTouch")
                        if endTouchPart then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = endTouchPart.CFrame
                        end
                    end
                end
            end
        end
    end
})

local teapotDodgeballTab = window:CreateTab("Teapot Dodgeball", 4483362458)

local scriptSectionTeapotDodgeball = teapotDodgeballTab:CreateSection("Script")

local winDodgeballButton = teapotDodgeballTab:CreateButton({
    Name = "Win Dodgeball",
    Callback = function()
        local teapotDodgeballModel = game.Workspace:FindFirstChild("TeapotDodgeball")
        if teapotDodgeballModel then
            local buildModel = teapotDodgeballModel:FindFirstChild("Build")
            if buildModel then
                local finishPart = buildModel:FindFirstChild("Finish")
                if finishPart then
                    local player = game.Players.LocalPlayer
                    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        player.Character.HumanoidRootPart.CFrame = finishPart.CFrame
                    end
                end
            end
        end
    end
})

local squishyFloodTab = window:CreateTab("Squishy Flood", 4483362458)

local scriptSectionSquishyFlood = squishyFloodTab:CreateSection("Script")

local instantWinSquishyFloodButton = squishyFloodTab:CreateButton({
    Name = "Instant Win",
    Callback = function()
        local funTimesModel = game.Workspace:FindFirstChild("FunTimesAtSquishyFlood")
        if funTimesModel then
            local buildModel = funTimesModel:FindFirstChild("Build")
            if buildModel then
                local winpartsFolder = buildModel:FindFirstChild("Winparts")
                if winpartsFolder then
                    local winParts = winpartsFolder:GetChildren()
                    local count = 0
                    for _, part in pairs(winParts) do
                        if part:IsA("Part") and part.Name == "WinPart" then
                            count = count + 1
                            if count == 4 then
                                local touchInterest = part:FindFirstChildOfClass("TouchTransmitter")
                                if touchInterest then
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part, 0)
                                    wait()
                                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part, 1)
                                end
                                break
                            end
                        end
                    end
                end
            end
        end
    end
})

local teleportToEndSquishyFloodButton = squishyFloodTab:CreateButton({
    Name = "Teleport To End",
    Callback = function()
        local funTimesModel = game.Workspace:FindFirstChild("FunTimesAtSquishyFlood")
        if funTimesModel then
            local buildModel = funTimesModel:FindFirstChild("Build")
            if buildModel then
                local winpartsFolder = buildModel:FindFirstChild("Winparts")
                if winpartsFolder then
                    local winParts = winpartsFolder:GetChildren()
                    local count = 0
                    for _, part in pairs(winParts) do
                        if part:IsA("Part") and part.Name == "WinPart" then
                            count = count + 1
                            if count == 4 then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
                                break
                            end
                        end
                    end
                end
            end
        end
    end
})

