local follyFightTab = window:CreateTab("Folly Fight", 4483362458)

local scriptSectionMirrors = follyFightTab:CreateSection("Mirrors")

local highlightFollyMirrorToggle = follyFightTab:CreateToggle({
    Name = "Highlight Folly Mirror",
    SectionParent = scriptSectionMirrors,
    Callback = function(enabled)
        while enabled do
            local follyFightModel = workspace:FindFirstChild("FollyFight")
            if follyFightModel then
                local buildModel = follyFightModel:FindFirstChild("Build")
                if buildModel then
                    local follyModel = buildModel:FindFirstChild("Folly")
                    if follyModel then
                        local follyMeshPart = follyModel:FindFirstChildWhichIsA("MeshPart")
                        if follyMeshPart then
                            local mirrorsFolder = buildModel:FindFirstChild("Mirrors")
                            if mirrorsFolder then
                                for _, follyMirrorModel in pairs(mirrorsFolder:GetChildren()) do
                                    if follyMirrorModel:IsA("Model") and follyMirrorModel.Name == "FollyMirror" then
                                        if follyMeshPart.Position.X >= follyMirrorModel:GetPivot().X - 5 and follyMeshPart.Position.X <= follyMirrorModel:GetPivot().X + 5 then
                                            if not follyMirrorModel:FindFirstChild("Highlight") then
                                                local highlight = Instance.new("Highlight")
                                                highlight.Parent = follyMirrorModel
                                            end
                                        else
                                            local existingHighlight = follyMirrorModel:FindFirstChild("Highlight")
                                            if existingHighlight then
                                                existingHighlight:Destroy()
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
            wait(0.1)
        end
    end
})

local pizzaTab = window:CreateTab("Pizza Delivery", 4483362458)

local pizzaSection = pizzaTab:CreateSection("Pizza")

local autoGetPizzasButton = pizzaTab:CreateButton({
    Name = "Auto Get Pizzas",
    SectionParent = pizzaSection,
    Callback = function()
        local pizzaBoxes = workspace.PizzaDelivery.Build:FindFirstChild("PizzaBoxes")
        if pizzaBoxes then
            local count = 0
            for _, meshPart in pairs(pizzaBoxes:GetChildren()) do
                if meshPart:IsA("MeshPart") and meshPart.Name == "PizzaBox" then
                    local touchTransmitter = meshPart:FindFirstChildWhichIsA("TouchTransmitter")
                    if touchTransmitter and count < 10 then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, touchTransmitter.Parent, 0)
                        wait()
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, touchTransmitter.Parent, 1)
                        count = count + 1
                    end
                end
            end
        end
    end
})

Rayfield:LoadConfiguration()
