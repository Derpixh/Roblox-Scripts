local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local window = Rayfield:CreateWindow({
    Name = "Bakon",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Derpixh",
    ConfigurationSaving = {
        Enabled = false,
        FileName = "Sirius"
    },
    KeySystem = true,
    KeySettings = {
        Title = "Derpixh's Key System",
        Subtitle = "Enter Your Key",
        Note = "Rate The Features",
        FileName = "Sirius",
        SaveKey = false,
        Key = "2025"
    }
})

local chapter1Tab = window:CreateTab("Chapter 1", 4483362458)
local mainStuffSection1 = chapter1Tab:CreateSection("Main Stuff")
chapter1Tab:CreateButton({
    Name = "Grab White Key",
    Callback = function()
        local CurrentMap = game.Workspace:FindFirstChild("CurrentMap")
        if CurrentMap then
            local Chapter1 = CurrentMap:FindFirstChild("Chapter1")
            if Chapter1 then
                local Utilities = Chapter1:FindFirstChild("Utilities")
                if Utilities then
                    local WhiteKey = Utilities:FindFirstChild("White Key")
                    if WhiteKey then
                        local Bounds = WhiteKey:FindFirstChild("Bounds")
                        if Bounds then
                            local ClickDetector = Bounds:FindFirstChild("PickUp")
                            if ClickDetector and ClickDetector:IsA("ClickDetector") then
                                fireclickdetector(ClickDetector)
                            end
                        end
                    end
                end
            end
        end
    end,
    SectionParent = mainStuffSection1
})

chapter1Tab:CreateButton({
    Name = "Grab USB",
    Callback = function()
        local CurrentMap = game.Workspace:FindFirstChild("CurrentMap")
        if CurrentMap then
