local teleportsSection = chapter1Tab:CreateSection("Teleports")
chapter1Tab:CreateButton({
    Name = "Teleport to Book Case",
    Callback = function()
        local CurrentMap = game.Workspace:FindFirstChild("CurrentMap")
        if CurrentMap then
            local Chapter1 = CurrentMap:FindFirstChild("Chapter1")
            if Chapter1 then
                local MIsc = Chapter1:FindFirstChild("MIsc")
                if MIsc then
                    local Furniture = MIsc:FindFirstChild("Furniture")
                    if Furniture then
                        local Carpets = Furniture:FindFirstChildWhichIsA("BasePart", 4)
                        if Carpets and Carpets.Name == "Carpet" then
                            local localPlayer = game.Players.LocalPlayer
                            local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
                            character:WaitForChild("HumanoidRootPart").CFrame = Carpets.CFrame
                        end
                    end
                end
            end
        end
    end,
    SectionParent = teleportsSection
})

chapter1Tab:CreateButton({
    Name = "Teleport to USB Computer",
    Callback = function()
        local CurrentMap = game.Workspace:FindFirstChild("CurrentMap")
        if CurrentMap then
            local Chapter1 = CurrentMap:FindFirstChild("Chapter1")
            if Chapter1 then
                local Scavengers = Chapter1:FindFirstChild("Scavengers")
                if Scavengers then
                    local FinalEscape = Scavengers:FindFirstChild("FinalEscape")
                    if FinalEscape then
                        local Computer = FinalEscape:FindFirstChild("Computer")
                        if Computer and Computer:IsA("Model") then
                            local localPlayer = game.Players.LocalPlayer
                            local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
                            character:SetPrimaryPartCFrame(Computer.PrimaryPart.CFrame)
                        end
                    end
                end
            end
        end
    end,
    SectionParent = teleportsSection
})

