local teleportsSection = chapter1Tab:CreateSection("Teleports")

chapter1Tab:CreateButton({
    Name = "Teleport to Book Case",
    Callback = function()
        local CurrentMap = game.Workspace:FindFirstChild("CurrentMap")
        if CurrentMap then
            local Chapter1 = CurrentMap:FindFirstChild("Chapter1")
            if Chapter1 then
                local Scavengers = Chapter1:FindFirstChild("Scavengers")
                if Scavengers then
                    local Bookcase = Scavengers:FindFirstChild("Bookcase")
                    if Bookcase then
                        local localPlayer = game.Players.LocalPlayer
                        local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
                        local frontOffset = Vector3.new(0, 0, -5)
                        local newPosition = Bookcase:GetModelCFrame().Position + frontOffset
                        character:WaitForChild("HumanoidRootPart").CFrame = CFrame.new(newPosition, Bookcase:GetModelCFrame().Position)
                    end
                end
            end
        end
    end,
    SectionParent = teleportsSection
})

chapter1Tab:CreateButton({
    Name = "Teleport to USB Computer",
    Callback = function()
        local CurrentMap = game.Workspace:FindFirstChild("CurrentMap")
        if CurrentMap then
            local Chapter1 = CurrentMap:FindFirstChild("Chapter1")
            if Chapter1 then
                local Scavengers = Chapter1:FindFirstChild("Scavengers")
                if Scavengers then
                    local FinalEscape = Scavengers:FindFirstChild("FinalEscape")
                    if FinalEscape then
                        local Computer = FinalEscape:FindFirstChild("Computer")
                        if Computer and Computer:IsA("Model") then
                            local localPlayer = game.Players.LocalPlayer
                            local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
                            character:SetPrimaryPartCFrame(Computer.PrimaryPart.CFrame)
                        end
                    end
                end
            end
        end
    end,
    SectionParent = teleportsSection
})

local chapter2Tab = window:CreateTab("Chapter 2", 4483362458)

chapter2Tab:CreateButton({
    Name = "Example Button",
    Callback = function()
        print("Example Button clicked!")
    end
})

Rayfield:LoadConfiguration()
