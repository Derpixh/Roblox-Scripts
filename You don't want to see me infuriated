})

local miscSection = stage14Tab:CreateSection("Misc")

local skipStageButtonStage14 = stage14Tab:CreateButton({
    Name = "Skip Stage (Easy Way)",
    SectionParent = miscSection,
    Callback = function()
        local obbyStages = workspace:FindFirstChild("ObbyStages")
        if obbyStages then
            local part15 = obbyStages:FindFirstChild("15")
            if part15 and part15:IsA("Part") then
                local touchInterest = part15:FindFirstChild("TouchInterest")
                if touchInterest then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part15, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part15, 1)
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part15.CFrame
            end
        end
    end
})

local stage15Tab = window:CreateTab("Stage 15")

local landminesSection = stage15Tab:CreateSection("Landmines")

local removeLandminesButton = stage15Tab:CreateButton({
    Name = "Remove Landmines",
    SectionParent = landminesSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder15 = stageParts:FindFirstChild("15")
            if folder15 then
                local landminesFolder = folder15:FindFirstChild("Landmines")
                if landminesFolder then
                    landminesFolder:Destroy()
                end
            end
        end
    end
})

local highlightCorrectDoorsToggle = stage15Tab:CreateToggle({
    Name = "Highlight Correct Doors (You Can Turn It Off/On)",
    SectionParent = doorsSection,
    CurrentValue = false,
    Callback = function(Value)
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder15 = stageParts:FindFirstChild("15")
            if folder15 then
                local doorsFolder = folder15:FindFirstChild("Doors")
                if doorsFolder then
                    local foldersToHighlight = {doorsFolder:FindFirstChild("1"), doorsFolder:FindFirstChild("2")}
                    for _, folder in ipairs(foldersToHighlight) do
                        if folder and folder:IsA("Folder") then
                            local part = folder:FindFirstChildOfClass("Part")
                            if part then
                                local existingHighlight = part:FindFirstChildOfClass("Highlight")
                                if Value then
                                    if not existingHighlight then
                                        local highlight = Instance.new("Highlight")
                                        highlight.FillColor = Color3.fromRGB(0, 255, 0)
                                        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
                                        highlight.Parent = part
                                    end
                                else
                                    if existingHighlight then
                                        existingHighlight:Destroy()
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
})

