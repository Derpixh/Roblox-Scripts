local stage14Tab = window:CreateTab("Stage 14")

local carsSection = stage14Tab:CreateSection("Cars")

local removeMovingCarsButton = stage14Tab:CreateButton({
    Name = "Remove Moving Cars",
    SectionParent = carsSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder14 = stageParts:FindFirstChild("14")
            if folder14 then
                local crushersFolder = folder14:FindFirstChild("Crushers")
                if crushersFolder then
                    crushersFolder:Destroy()
                end
            end
        end
    end
})

local landminesSection = stage14Tab:CreateSection("Landmines")

local removeLandminesButton = stage14Tab:CreateButton({
    Name = "Remove Landmines",
    SectionParent = landminesSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder14 = stageParts:FindFirstChild("14")
            if folder14 then
                local landminesFolder = folder14:FindFirstChild("Landmines")
                if landminesFolder then
                    landminesFolder:Destroy()
                end
            end
        end
    end
})

local axesSection = stage14Tab:CreateSection("Axes")

local removeSwingingAxesButton = stage14Tab:CreateButton({
    Name = "Remove Swinging Axes",
    SectionParent = axesSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder14 = stageParts:FindFirstChild("14")
            if folder14 then
