local stage11Tab = window:CreateTab("Stage 11")

local mainSection = stage11Tab:CreateSection("Main")

local removeFruitsToggle = stage11Tab:CreateToggle({
    Name = "Remove Fruits (You Can Add Them Back Too With The Toggle)",
    SectionParent = mainSection,
    CurrentValue = false,
    Flag = "RemoveFruits",
    Callback = function(Value)
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder11 = stageParts:FindFirstChild("11")
            if folder11 then
                local button = folder11:FindFirstChild("Button")
                if button and button:IsA("Part") then
                    local touchInterest = button:FindFirstChild("TouchInterest")
                    if touchInterest then
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, button, 0)
                        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, button, 1)
                    end
                end
            end
        end
    end,
})

