local stage8Tab = window:CreateTab("Stage 8")

local sigmundSection = stage8Tab:CreateSection("Sigmund")

local removeSigmundsButton = stage8Tab:CreateButton({
    Name = "Remove Sigmunds",
    SectionParent = sigmundSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder8 = stageParts:FindFirstChild("8")
            if folder8 then
                for _, part in ipairs(folder8:GetChildren()) do
                    if part.Name == "Sigmund" and part:IsA("Part") then
                        part:Destroy()
                    end
                end
            end
        end
    end
})

local wedgesSection = stage8Tab:CreateSection("Wedges")

local removeWedgesButton = stage8Tab:CreateButton({
    Name = "Remove Wedges",
    SectionParent = wedgesSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder8 = stageParts:FindFirstChild("8")
            if folder8 then
                local killBricksFolder = folder8:FindFirstChild("KillBricks")
                if killBricksFolder then
                    for index, wedge in ipairs(killBricksFolder:GetChildren()) do
                        if wedge.Name == "Wedge" and wedge:IsA("Part") and index ~= 1 and index ~= 13 then
                            wedge:Destroy()
                        end
                    end
                end
            end
        end
    end
})

local landminesSection = stage8Tab:CreateSection("Landmines")

local removeLandminesButton = stage8Tab:CreateButton({
    Name = "Remove Landmines",
    SectionParent = landminesSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder8 = stageParts:FindFirstChild("8")
            if folder8 then
                local landminesFolder = folder8:FindFirstChild("Landmines")
                if landminesFolder then
                    landminesFolder:Destroy()
                end
            end
        end
    end
})

local killbricksSection = stage8Tab:CreateSection("KillBricks")

local invincibilityKillbricksButton = stage8Tab:CreateButton({
    Name = "Invincibility To KillBricks",
    SectionParent = killbricksSection,
    Callback = function()
