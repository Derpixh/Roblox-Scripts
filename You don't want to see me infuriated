local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local window = Rayfield:CreateWindow({
    Name = "You don't wanna see me infuriated - Alpha Hax",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Derpixh",
    ConfigurationSaving = {
        Enabled = false,
        FileName = "Sirius"
    },
    KeySystem = true,
    KeySettings = {
        Title = "Derpixh's Key System",
        Subtitle = "Enter Your Key",
        Note = "Rate The Features",
        FileName = "Sirius",
        SaveKey = false,
        Key = "2025"
    }
})

local stage1Tab = window:CreateTab("Stage 1")

local obbySection = stage1Tab:CreateSection("Obby")

local freezeObbyButton = stage1Tab:CreateButton({
    Name = "Freeze Obby (Easier To Walk Across)",
    SectionParent = obbySection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder1 = stageParts:FindFirstChild("1")
            if folder1 then
                local reallyGoodBridge = folder1:FindFirstChild("ReallyGoodBridge")
                if reallyGoodBridge then
                    local bridgeParts = reallyGoodBridge:FindFirstChild("BridgeParts")
                    if bridgeParts then
                        for _, part in ipairs(bridgeParts:GetChildren()) do
                            if part.Name == "Baseplate" and part:IsA("Part") then
                                part.Anchored = true
                            end
                        end
                    end
                end
            end
        end
    end
})

local miscStage1Section = stage1Tab:CreateSection("Misc")

local skipStageButtonStage1 = stage1Tab:CreateButton({
    Name = "Skip Stage (Easy Way)",
    SectionParent = miscStage1Section,
    Callback = function()
        local obbyStages = workspace:FindFirstChild("ObbyStages")
        if obbyStages then
            local part1 = obbyStages:FindFirstChild("1")
            if part1 and part1:IsA("Part") then
                local touchInterest = part1:FindFirstChild("TouchInterest")
                if touchInterest then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part1, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part1, 1)
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part1.CFrame
            end
        end
    end
})

local stage2Tab = window:CreateTab("Stage 2")

local quizDoorsSection = stage2Tab:CreateSection("Quiz Doors")

local openQuizDoorsButton = stage2Tab:CreateButton({
    Name = "Open Quiz Doors (You Can Walk Through The Doors)",
    SectionParent = quizDoorsSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder2 = stageParts:FindFirstChild("2")
            if folder2 then
                local doorsFolder = folder2:FindFirstChild("Doors")
                if doorsFolder then
                    for _, part in ipairs(doorsFolder:GetChildren()) do
                        if part.Name == "Quiz Door" and part:IsA("Part") then
                            part.Transparency = 1
                            part.CanCollide = false
                        end
                    end
                end
            end
        end
    end
})

local miscStage2Section = stage2Tab:CreateSection("Misc")

local skipStageButtonStage2 = stage2Tab:CreateButton({
    Name = "Skip Stage (Easy Way)",
    SectionParent = miscStage2Section,
    Callback = function()
        local obbyStages = workspace:FindFirstChild("ObbyStages")
        if obbyStages then
            local part2 = obbyStages:FindFirstChild("2")
            if part2 and part2:IsA("Part") then
                local touchInterest = part2:FindFirstChild("TouchInterest")
                if touchInterest then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part2, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part2, 1)
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part2.CFrame
            end
        end
    end
})

local stage3Tab = window:CreateTab("Stage 3")

local landminesSection = stage3Tab:CreateSection("Landmines")

local landminesButton = stage3Tab:CreateButton({
    Name = "Remove Landmines (Easier Gameplay)",
    SectionParent = landminesSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder3 = stageParts:FindFirstChild("3")
            if folder3 then
                local landminesFolder = folder3:FindFirstChild("Landmines")
                if landminesFolder then
                    for _, landmine in ipairs(landminesFolder:GetChildren()) do
                        if landmine:IsA("Part") then
                            landmine:Destroy()
                        end
                    end
                end
            end
        end
    end
})

local miscStage3Section = stage3Tab:CreateSection("Misc")

local skipStageButtonStage3 = stage3Tab:CreateButton({
    Name = "Skip Stage (Easy Way)",
    SectionParent = miscStage3Section,
    Callback = function()
        local obbyStages = workspace:FindFirstChild("ObbyStages")
        if obbyStages then
            local part4 = obbyStages:FindFirstChild("4")
            if part4 and part4:IsA("Part") then
                local touchInterest = part4:FindFirstChild("TouchInterest")
                if touchInterest then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part4, 0)
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, part4, 1)
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part4.CFrame
            end
        end
    end
})

