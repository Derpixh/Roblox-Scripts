})

local carSpawnerSection = stage8Tab:CreateSection("Car Spawner")

local spawnCarButton = stage8Tab:CreateButton({
    Name = "Spawn Car From Spawner 1",
    SectionParent = carSpawnerSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder8 = stageParts:FindFirstChild("8")
            if folder8 then
                local spawnersFolder = folder8:FindFirstChild("Spawners")
                if spawnersFolder then
                    local spawner = spawnersFolder:GetChildren()[4]
                    if spawner and spawner.Name == "Spawner" and spawner:IsA("Part") then
                        local touchInterest = spawner:FindFirstChild("TouchInterest")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, spawner, 0)
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, spawner, 1)
                        end
                    end
                end
            end
        end
    end
})

