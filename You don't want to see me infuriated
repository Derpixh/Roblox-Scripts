                if axesFolder then
                    axesFolder:Destroy()
                end
            end
        end
    end
})

local carSpawnerSection = stage14Tab:CreateSection("Car Spawner")

local spawnCarButton = stage14Tab:CreateButton({
    Name = "Spawn Car From Spawner 1",
    SectionParent = carSpawnerSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder14 = stageParts:FindFirstChild("14")
            if folder14 then
                local spawnersFolder = folder14:FindFirstChild("Spawners")
                if spawnersFolder then
                    local spawner = spawnersFolder:GetChildren()[3]
                    if spawner and spawner.Name == "Spawner" and spawner:IsA("Part") then
                        local touchInterest = spawner:FindFirstChild("TouchInterest")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, spawner, 0)
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, spawner, 1)
                        end
                    end
                end
            end
        end
    end
})

local spawnCarButton2 = stage14Tab:CreateButton({
    Name = "Spawn Car From Spawner 2",
    SectionParent = carSpawnerSection,
    Callback = function()
        local stageParts = workspace:FindFirstChild("StageParts")
        if stageParts then
            local folder14 = stageParts:FindFirstChild("14")
            if folder14 then
                local spawnersFolder = folder14:FindFirstChild("Spawners")
                if spawnersFolder then
                    local spawner = spawnersFolder:GetChildren()[1]
                    if spawner and spawner.Name == "Spawner" and spawner:IsA("Part") then
                        local touchInterest = spawner:FindFirstChild("TouchInterest")
                        if touchInterest then
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, spawner, 0)
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, spawner, 1)
                        end
                    end
                end
            end
        end
    end
})

local spawnCarButton3 = stage14Tab:CreateButton({
    Name = "Spawn Car From Spawner 3",
