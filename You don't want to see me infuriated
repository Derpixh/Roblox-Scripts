                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part6.CFrame
            end
        end
    end
})

local stage6Tab = window:CreateTab("Stage 6")

local captchaSection = stage6Tab:CreateSection("Captcha")

local captchaToggleActive = false

local autoDisableCaptchaToggle = stage6Tab:CreateToggle({
    Name = "Automatically Disable Captcha (Annoying Popup)",
    SectionParent = captchaSection,
    CurrentValue = false,
    Flag = "AutoDisableCaptcha",
    Callback = function(Value)
        captchaToggleActive = Value
        if captchaToggleActive then
            spawn(function()
                while captchaToggleActive do
                    wait()
                    local players = game:GetService("Players")
                    local localPlayer = players.LocalPlayer
                    if localPlayer then
                        local playerGui = localPlayer:FindFirstChild("PlayerGui")
                        if playerGui then
                            local captchaGui = playerGui:FindFirstChild("Captcha")
                            if captchaGui and captchaGui:IsA("ScreenGui") then
                                if captchaGui.Enabled then
                                    captchaGui.Enabled = false
                                end
                            end
                        end
                    end
                end
            end)
        end
    end,
})

local walkspeedSection = stage6Tab:CreateSection("Walkspeed")

local fixWalkspeedToggleActive = false

local autoFixWalkspeedToggle = stage6Tab:CreateToggle({
    Name = "Automatically Fix Walkspeed (You Need To Turn It Off when You Get To Stage 7)",
    SectionParent = walkspeedSection,
    CurrentValue = false,
    Flag = "AutoFixWalkspeed",
    Callback = function(Value)
        fixWalkspeedToggleActive = Value
        if fixWalkspeedToggleActive then
            spawn(function()
                while fixWalkspeedToggleActive do
                    wait()
                    local players = game:GetService("Players")
                    local localPlayer = players.LocalPlayer
                    if localPlayer then
                        local playerModel = workspace:FindFirstChild(localPlayer.Name)
                        if playerModel and playerModel:IsA("Model") then
                            local humanoid = playerModel:FindFirstChild("Humanoid")
                            if humanoid and humanoid:IsA("Humanoid") then
                                if humanoid.WalkSpeed ~= 16 then
                                    humanoid.WalkSpeed = 16
                                end
                            end
                        end
                    end
                end
            end)
        end
    end,
})

