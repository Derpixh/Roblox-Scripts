local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local window = Rayfield:CreateWindow({
    Name = "I Wanna Test The Game",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Derpixh",
    ConfigurationSaving = {
        Enabled = false,
        FileName = "Sirius"
    },
    KeySystem = true,
    KeySettings = {
        Title = "Key System",
        Subtitle = "Key Prompt",
        Note = "Enter your key",
        FileName = "SiriusKey",
        SaveKey = false,
        Key = "2025"
    }
})

local stage1Tab = window:CreateTab("Stage 1")
local scriptSection = stage1Tab:CreateSection("Script")

local removeLavasButton = stage1Tab:CreateButton({
    Name = "Remove Lavas",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local lavaStageModel = stageFolder:FindFirstChild("LavaStage")
                if lavaStageModel then
                    local lavasModel = lavaStageModel:FindFirstChild("Lavas")
                    if lavasModel then
                        lavasModel:Destroy()
                    end
                end
            end
        end
    end
})

Rayfield:LoadConfiguration()
