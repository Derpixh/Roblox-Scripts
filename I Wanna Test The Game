local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local window = Rayfield:CreateWindow({
    Name = "I Wanna Test The Game",
    LoadingTitle = "Loading Assets..",
    LoadingSubtitle = "Created By Derpixh",
    ConfigurationSaving = {
        Enabled = false,
        FileName = "Sirius"
    },
    KeySystem = true,
    KeySettings = {
        Title = "Key System",
        Subtitle = "Key Prompt",
        Note = "Enter your key",
        FileName = "SiriusKey",
        SaveKey = false,
        Key = "2025"
    }
})

local stage1Tab = window:CreateTab("Stage 1")
local scriptSectionStage1 = stage1Tab:CreateSection("Script")
local removeLavasButton = stage1Tab:CreateButton({
    Name = "Remove Lavas (Blatant/Lazy Method)",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local lavaStageModel = stageFolder:FindFirstChild("LavaStage")
                if lavaStageModel then
                    local lavasModel = lavaStageModel:FindFirstChild("Lavas")
                    if lavasModel then
                        lavasModel:Destroy()
                    end
                end
            end
        end
    end
})

local removeTriggersLegitButton = stage1Tab:CreateButton({
    Name = "Remove Triggers (Legit/Easy Method)",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local lavaStageModel = stageFolder:FindFirstChild("LavaStage")
                if lavaStageModel then
                    local lavasModel = lavaStageModel:FindFirstChild("Lavas")
                    if lavasModel then
                        for i = 1, 4 do
                            local lavaModel = lavasModel:FindFirstChild("Lava" .. i)
                            if lavaModel then
                                local triggerPart = lavaModel:FindFirstChild("Trigger")
                                if triggerPart then
                                    triggerPart:Destroy()
                                end
                            end
                        end
                    end
                end
            end
        end
    end
})

local stage2Tab = window:CreateTab("Stage 2")
local scriptSectionStage2 = stage2Tab:CreateSection("Script")
local removeTriggersButton = stage2Tab:CreateButton({
    Name = "Remove Triggers (Legit/Easy Method)",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local lavaStage2Model = stageFolder:FindFirstChild("LavaStage2")
                if lavaStage2Model then
                    local triggersModel = lavaStage2Model:FindFirstChild("Triggers")
                    if triggersModel then
                        triggersModel:Destroy()
                    end
                end
            end
        end
    end
})

local stage3Tab = window:CreateTab("Stage 3")
local firstPartSectionStage3 = stage3Tab:CreateSection("First Part Of Stage 3")
local freezeObbyKillersButton = firstPartSectionStage3:CreateButton({
    Name = "Freeze Obby Killers (Disable Movement)",
    Callback = function()
        local npcsFolder = game.Workspace:FindFirstChild("NPCs")
        if npcsFolder then
            local obbyKiller1 = npcsFolder:FindFirstChild("ObbyKiller_1")
            if obbyKiller1 then
                local hitbox1 = obbyKiller1:FindFirstChild("Hitbox")
                if hitbox1 then
                    hitbox1.Anchored = true
                end
            end
            local obbyKiller2 = npcsFolder:FindFirstChild("ObbyKiller_2")
            if obbyKiller2 then
                local hitbox2 = obbyKiller2:FindFirstChild("Hitbox")
                if hitbox2 then
                    hitbox2.Anchored = true
                end
            end
        end
    end
})

local lavaObbySectionStage3 = stage3Tab:CreateSection("Lava Obby")
local removeLavaButton = lavaObbySectionStage3:CreateButton({
    Name = "Remove Lava On Obby (Easy Method)",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local stage3Model = stageFolder:FindFirstChild("Stage3")
                if stage3Model then
                    local lavaModel = stage3Model:FindFirstChild("Lava")
                    if lavaModel then
                        lavaModel:Destroy()
                    end
                end
            end
        end
    end
})

local invincibilityToLavaObbyButton = lavaObbySectionStage3:CreateButton({
    Name = "Invincibility To Lava Obby (Easiest Method)",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local stage3Model = stageFolder:FindFirstChild("Stage3")
                if stage3Model then
                    local lavaModel = stage3Model:FindFirstChild("Lava")
                    if lavaModel then
                        for _, lavaPart in ipairs(lavaModel:GetChildren()) do
                            if lavaPart:IsA("Part") and lavaPart.Name == "Lava" then
                                local touchInterest = lavaPart:FindFirstChild("TouchInterest")
                                if touchInterest then
                                    touchInterest:Destroy()
                                end
                            end
                        end
                    end
                end
            end
        end
    end
})

local lastPartSectionStage3 = stage3Tab:CreateSection("Last Part Of Stage 3")
local removeTriggersButtonStage3 = lastPartSectionStage3:CreateButton({
    Name = "Remove Triggers (Last Part Of Stage 3)",
    Callback = function()
        local mapFolder = game.Workspace:FindFirstChild("Map")
        if mapFolder then
            local stageFolder = mapFolder:FindFirstChild("Stage")
            if stageFolder then
                local stage3Model = stageFolder:FindFirstChild("Stage3")
                if stage3Model then
                    local triggersModel = stage3Model:FindFirstChild("Triggers")
                    if triggersModel then
                        triggersModel:Destroy()
                    end
                end
            end
        end
    end
})

Rayfield:LoadConfiguration()
